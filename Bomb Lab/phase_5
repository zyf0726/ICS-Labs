s = input_string()
assert(s.length() == 6)

sum = 0
for i = 0 to 5 do {
	idx = s[i] & 0xF
	sum += arr[idx]
}
assert(sum == 0x33)


arr = [ 2, 10,  6,  1,
	   12, 16,  9,  3,
	    4,  7, 14,  5,
	   11,  8, 15, 13]
	   
	   
6 + 7 + 8 + 9 + 10 + 11 = 51 = 0x33
sum of arr[{1, 2, 6, 9, 12, 13}] = 51





